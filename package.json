{
  "name": "@breautek/storm",
  "version": "0.19.3",
  "description": "Object-Oriented REST API framework",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": "src/**/*.ts",
    "extension": [
      ".ts"
    ]
  },
  "scripts": {
    "build": "tsc && npm run-script build-docs",
    "build-lib": "rm -r ./lib && tsc",
    "build-docs": "typedoc --readme ./README.md --out ./docs ./src",
    "lint": "eslint --ext .ts './src/**/*.ts'",
    "unit": "jasmine-ts",
    "test": "npm run-script lint && nyc --reporter=lcov --reporter=text jasmine-ts",
    "coverage": "codecov",
    "clean": "rm -r ./lib && rm -r ./docs",
    "prepublishOnly": "npm run-script build-lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breautek/storm.git"
  },
  "keywords": [
    "web",
    "api",
    "storm",
    "rest",
    "object",
    "oriented",
    "breautek"
  ],
  "author": "norman@normanbreau.com",
  "license": "GPL-3.0",
  "homepage": "https://github.com/breautek/storm.git",
  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "^4.17.0",
    "@types/formidable": "1.0.31",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.6.3",
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.19.0",
    "commander": "^2.20.0",
    "express": "^4.17.1",
    "formidable": "1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "source-map-support": "^0.5.12",
    "tslib": "^1.10.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@breautek/eslint-plugin": "^1.0.0",
    "@breautek/jasmine-ts": "1.0.0",
    "@types/jasmine": "^3.3.13",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "codecov": "^3.5.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "form-data": "^2.5.0",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tsutils": "^3.14.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.5.3"
  }
}
