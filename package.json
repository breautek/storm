{
  "name": "@breautek/storm",
  "version": "1.2.2",
  "description": "Object-Oriented REST API framework",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": "src/**/*.ts",
    "extension": [
      ".ts"
    ],
    "lines": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ]
    },
    "exclude": [
      "**/*Middleware*"
    ]
  },
  "scripts": {
    "build": "npm run lint && tsc && npm run-script build-docs",
    "build-lib": "rm -r ./lib && tsc",
    "build-docs": "typedoc --excludePrivate --excludeNotExported --mode file -readme ./README.md --out ./docs ./src",
    "lint": "eslint --ext .ts '?(src|spec)/**/*.ts' --cache",
    "unit": "ts-node ./spec/support/jasmine.ts",
    "test": "npm run-script lint && nyc --reporter=lcov --reporter=text ts-node ./spec/support/jasmine.ts",
    "coverage": "codecov",
    "clean": "rm -r ./lib && rm -r ./docs",
    "prepublishOnly": "npm run-script build-lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breautek/storm.git"
  },
  "keywords": [
    "web",
    "api",
    "storm",
    "rest",
    "object",
    "oriented",
    "breautek"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "norman@normanbreau.com",
  "license": "GPL-3.0",
  "homepage": "https://github.com/breautek/storm.git",
  "dependencies": {
    "@breautek/iterator": "1.0.2",
    "@types/body-parser": "1.17.1",
    "@types/express": "4.17.2",
    "@types/formidable": "1.0.31",
    "@types/jsonwebtoken": "8.3.7",
    "@types/mysql": "2.15.8",
    "@types/node": "12.12.26",
    "@types/uuid": "3.4.7",
    "body-parser": "1.19.0",
    "commander": "2.20.0",
    "express": "4.17.1",
    "formidable": "1.2.1",
    "jsonwebtoken": "8.5.1",
    "mysql": "2.18.1",
    "source-map-support": "0.5.16",
    "tslib": "1.10.0",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@breautek/eslint-plugin": "3.0.0",
    "@breautek/jasmine": "3.5.1",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "codecov": "3.6.4",
    "eslint": "6.8.0",
    "form-data": "2.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jasmine-ts-console-reporter": "3.1.1",
    "nyc": "15.0.0",
    "ts-node": "8.6.2",
    "tsutils": "3.17.1",
    "typedoc": "0.16.9",
    "typescript": "3.7.5"
  }
}
